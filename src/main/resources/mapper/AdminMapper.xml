<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.VisaAPI.mapper.AdminMapper">
 
    <select id="selectAll" resultType="com.example.VisaAPI.model.AdminModel">
        SELECT * FROM visa_user_api ;
    </select>
<select id="selectuser" resultType="com.example.VisaAPI.model.AdminModel">
        SELECT * FROM visa_user_api where username=#{username} ;
    </select>
</mapper>